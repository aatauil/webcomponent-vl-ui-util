{
  "name": "vl-ui-util",
  "version": "5.0.10",
  "description": "De util verzamelt verschillende handige webcomponent scripts.",
  "main": "index.js",
  "author": "DIDM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-util.git"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-util#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "config": {
    "script-shell": "bash"
  },
  "scripts": {
    "util": "npm run install:sass && npm run install:copy && npm run install:css:minify && npm run install:fonts && npm run install:bamboo && npm run install:copy:github && npm run update:readme",
    "install:utils": "npm run install:bamboo && npm run install:copy:install && npm run install:copy:codeowner && npm run install:copy:github && npm run install:performance",
    "install:bamboo": "node ./bamboo/bamboo-spec.js",
    "install:sass": "npm run install:sass:webcomponent && npm run install:sass:demo",
    "install:sass:webcomponent": "sass ../../src/style.scss ../../src/style.css --load-path ../../node_modules/@govflanders",
    "install:sass:demo": "sass demo.scss ../../demo/demo.css --load-path ../../node_modules/@govflanders",
    "install:performance": "node ./performance/generate.js --iterations=100",
    "install:copy": "npm run install:copy:install && npm run install:copy:test && npm run install:copy:release && npm run install:copy:demo:js && npm run install:copy:demo:css && npm run install:copy:prismjs && npm run install:copy:codeowner && npm run install:copy:github && npm run install:copy:wct && npm install:copy:performance",
    "install:copy:install": "npm i copyfiles@2.1.0 --save",
    "install:copy:wct": "cp *.conf.json ../../",
    "install:copy:test": "copyfiles -f test/docker-compose.yml ../../test",
    "install:copy:release": "copyfiles -f docker-compose-release.yml ../../",
    "install:copy:demo:js": "copyfiles -f demo.js ../../demo",
    "install:copy:codeowner": "copyfiles CODEOWNERS ../../",
    "install:copy:github": "cp -R .github ../../",
    "install:copy:demo:css": "copyfiles -f demo.css ../../demo",
    "install:copy:prismjs": "copyfiles -f 'node_modules/prismjs/**' ../../node_modules/prismjs",
    "install:copy:performance": "copyfiles -f performance/** ../../performance && copyfiles -f docker-compose-performance.yml ../../",
    "install:css:minify": "minify ../../src/style.css > ../../src/style.min.css && cp ../../src/style.min.css ../../src/style.css && rm ../../src/style.min.css",
    "install:fonts": "npm run install:font:path-sans && npm run install:font:path-serif && npm run install:font:path-italic && npm run install:font:vlaanderen-icon",
    "install:font:path-sans": "replace 'https://ui.vlaanderen.be/assets/fonts/sans/FlandersArtSans' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/flandersart/FlandersArtSans' ../../src/style.css",
    "install:font:path-serif": "replace 'https://ui.vlaanderen.be/assets/fonts/serif/FlandersArtSerif' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/flandersart/FlandersArtSerif' ../../src/style.css",
    "install:font:path-italic": "replace 'https://ui.vlaanderen.be/assets/fonts/italic/FlandersArtSans' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/flandersart/FlandersArtSansItalic' ../../src/style.css",
    "install:font:vlaanderen-icon": "replace '/font/iconfont/vlaanderen-icon.woff' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/iconfont/3.9.1/vlaanderen-icon.woff' ../../src/style.css",
    "pretest": "node build test/single/example example no-commit && node build test/multiple/example example no-commit ",
    "test": "mocha",
    "update:readme": "bash ./scripts/updateReadme.sh",
    "release": "np",
    "release:prepare": "echo 2Fabiola for the bigger and bolder",
    "release-component:prepare": "node build",
    "release:testless": "np --yolo",
    "explode": "node exploder.js"
  },
  "dependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chromedriver": "80.0.1",
    "event-stream": "^4.0.1",
    "fetch-mock": "^8.1.0",
    "geckodriver": "1.19.1",
    "http-server": "0.12.1",
    "jsdom": "16.2.1",
    "minify": "^5.0.0",
    "mocha": "^7.1.0",
    "np": "4.0.2",
    "prismjs": "1.17.1",
    "replace": "1.1.5",
    "sass": "1.24.4",
    "selenium-webdriver": "4.0.0-alpha.5",
    "sinon": "7.5.0",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2",
    "yaml": "1.7.2",
    "yargs": "15.0.2"
  },
  "devDependencies": {
    "copyfiles": "2.2.0",
    "eslint": "^6.3.0",
    "octonode": "^0.9.5"
  }
}
